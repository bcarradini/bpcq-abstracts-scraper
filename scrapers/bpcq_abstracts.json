{
  "url": ".*sagepub.*\\.com",
  "headless": true,
  "elements": {
    "title": {
      "selector": "//meta[@name='dc.Title']",
      "attribute": "content"
    },
    "author": {
      "selector": "//meta[@name='dc.Creator']",
      "attribute": "content"
    },
    "abstract": {
      "selector": "//meta[@name='dc.Description']",
      "attribute": "content"
    }
  }
}